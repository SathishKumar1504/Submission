#!javascript

function gen(n){
return {
    [Symbol.iterator](){
        let i=0;
        return {
            next(){
                Return {done: (i>n)? true:false, value: i++}    
                if(i < n){
                    return {value: i++, done:false};
                }else{
                    return {value:undefined, done:true};
                }
            }
        };
    }
};
}


function gen(n){
    return {
        [Symbol.iterator](){
            let i=0;
            return {
                next(){
                    if(i < n){
                        return {value: i++, done:false};
                    }else{
                        return {value:undefined, done:true};
                    }
                }
            };
        }
    };
}

genObj = gen(5);
for (let val of genObj){
    console.log(val);
}

