#!javascript

function* flatten(array){
    for (let item of array){
        if (item instanceof Array){
            yield* flatten(item);
        }else{
            yield item;
        }
    }
}
let t = flatten([1,[2,[3,4],5],6]);
console.log(t);
console.log(t.next());
console.log(t.next());
console.log(t.next());
console.log(t.next());
console.log(t.next());

