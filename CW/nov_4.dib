#!javascript

class jedi{
constructor (){
this.forceIsDark=false;
}
toString(){
    return (this.forceIsDark)? "Dark Side":"Light Side";
}
}
class sith extends jedi{
    constructor(){
        super();
        this.forceIsDark=true;
    }
    toString(){
        return (this.forceIsDark)? "Dark Side Sith":"Light Side Sith";
    }
}

let jedi1 = new jedi();
console.log(`${jedi1}`);
let sith1 = new sith();
console.log(`${sith1}`);

console.log(sith1.__proto_ );
console.log(sith1.__proto__.__proto__ );
console.log(sith1 instanceof jedi);
console.log(jedi1 instanceof sith);

